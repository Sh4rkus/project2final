<% include header %>
<div id="aboutHeader">
    <h2>Natures</h2>
</div>

<% if(rs.length > 0) { %>
<table class="table table-bordered table-striped">
    <tr><th>Nature Name</th><th>Positive</th><th>Negative</th></tr>
    <% for (var i=0; rs.length > i; i++) { %>
    <tr>
        <td><%= rs[i].NatureName %></td>
        <td><%= rs[i].Positive %></td>
        <td><%= rs[i].Negative %></td>
    </tr>
    <% } %>
</table>
<% } else { %>
No users are entered into the database.
<% } %>

<% include footer %>